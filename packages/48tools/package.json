    "cookie": "^0.7.0",
    "dayjs": "^1.11.10",
    "filenamify": "^6.0.0",
    "hls.js": "^1.4.14",
    "mpegts.js": "^1.7.3",
    "nim-web-sdk-ng": "^0.17.0",
    "path-to-regexp": "^8.1.0",
    "qrcode": "^1.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.0.4",
